    # src[geglu.py:N]:     a_vals = a_flat[tile_idx].to(torch.float32)
    # src[geglu.py:N-N]: ...
    _launcher(_helion_geglu, (triton.cdiv(1048576, _BLOCK_SIZE_0),), a_flat, b_flat, out_flat, _BLOCK_SIZE_0, num_warps=4, num_stages=3)
